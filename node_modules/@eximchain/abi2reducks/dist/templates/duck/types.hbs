import { MethodAbi } from 'ethereum-types';

export enum ParamNames {
{{#each inputs}}
{{#if name}}
    {{name}} = '{{name}}',
{{else}}
    param{{@index}} = 'param{{@index}}',
{{/if}}
{{/each}}
}

export type Input = {
{{#each inputs}}
{{#if name}}
    {{name}} : {{solToJsType type}}
{{else}}
    param{{@index}} : {{solToJsType type}}
{{/if}}
{{/each}}
}

export const Method:MethodAbi = {{{ stringify methodAbi }}}